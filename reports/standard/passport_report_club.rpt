{
	SQL => "
    SELECT DISTINCT
        P.intPassportID AS PassportID,
        'Club' AS EntityType,
        C.strName AS EntityName,
        P.dtLastlogin AS DateLastLogin,
        P.dtCreated AS DateAdded
    FROM
        tblPassportAuth AS P
        INNER JOIN tblClub AS C ON ( P.intEntityTypeID=3 AND P.intEntityID=C.intClubID )
        LEFT JOIN tblAssoc_Clubs AS AC ON ( C.intClubID=AC.intClubID AND AC.intRecStatus != -1 )
    WHERE
        ( AC.intAssocID = ###ASSOCID### AND ###ASSOCID### > 0 AND ###CLUBID### <= 0 )
        OR ( C.intClubID = ###CLUBID### AND ###CLUBID### > 0 )
	",
	StatsReport => 0,
	MemberTeam => 0,
	ReportEntity => 0,
	ReportLevel => 0,
	Template => 'default',
	DistinctValues => 1,
    TalkToPassport => 1,
	Labels => [
		['EntityType',   'EntityType'],
		['EntityName',   'EntityName'],
		['PassportID',   'PassportID'],
		['FirstName' ,   'FirstName' ],
		['FamilyName',   'FamilyName'],
		['Email',        'Email'     ],
		['DateCreated',  'DateCreated'  ],
		['DateAdded',    'DateAdded'    ],
		['DateLastLogin','DateLastLogin'],
	],
	Sort => [
		['EntityName','ASC','string'],
	],
}
