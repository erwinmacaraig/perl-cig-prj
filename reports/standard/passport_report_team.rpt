{
	SQL => "
    SELECT DISTINCT
        P.intPassportID AS PassportID,
        'Team' AS EntityType,
        T.strName AS EntityName,
        P.dtLastlogin AS DateLastLogin,
        P.dtCreated AS DateAdded
    FROM
        tblPassportAuth AS P
        INNER JOIN tblTeam AS T ON ( P.intEntityTypeID=2 AND P.intEntityID=T.intTeamID )
        LEFT JOIN tblComp_Teams AS CT ON ( T.intTeamID=CT.intTeamID AND CT.intRecStatus != -1 )
        LEFT JOIN tblAssoc_Comp AS AC ON ( CT.intCompID=AC.intCompID AND AC.intRecStatus != -1 )
    WHERE
 IF(###TEAMID###>0,
    T.intTeamID = ###TEAMID###,
    (IF(###CLUBID###>0,
       T.intClubID = ###CLUBID###,
       AC.intAssocID = ###ASSOCID###
     )
   )
  )
",
	StatsReport => 0,
	MemberTeam => 0,
	ReportEntity => 0,
	ReportLevel => 0,
	Template => 'default',
	DistinctValues => 1,
    TalkToPassport => 1,
	Labels => [
		['EntityType',   'EntityType'],
		['EntityName',   'EntityName'],
		['PassportID',   'PassportID'],
		['FirstName' ,   'FirstName' ],
		['FamilyName',   'FamilyName'],
		['Email',        'Email'     ],
		['DateCreated',  'DateCreated'  ],
		['DateAdded',    'DateAdded'    ],
		['DateLastLogin','DateLastLogin'],
	],
	Sort => [
		['EntityName','ASC','string'],
	],
}
