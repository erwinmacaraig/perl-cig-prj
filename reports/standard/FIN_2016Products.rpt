{
	SQL => qq[

(SELECT Region.strLocalName as RegionName, P.strName as ProductName, P.strProductType, P.curDefaultAmount, SUM(T.intQty) as SumQty, SUM(T.curAmount) as SumAmt FROM tblPersonRegistration_1 as PR INNER JOIN tblEntity as E ON (E.intEntityID = PR.intEntityID) INNER JOIN tblTransactions as T ON (PR.intPersonRegistrationID = T.intPersonRegistrationID) INNER JOIN tblTransLog as TL ON (TL.intLogID = T.intTransLogID) INNER JOIN tblProducts as P ON (P.intProductID=T.intProductID) INNER JOIN tblTempEntityStructure as TS ON (intParentLevel=20 and TS.intChildID = PR.intEntityID) INNER JOIN tblEntity as Region ON (Region.intEntityID= intParentID AND intParentLevel=20) WHERE T.intStatus=1 AND P.intProductNationalPeriodID=121 AND PR.intNationalPeriodID=121 GROUP BY RegionName, P.intProductID)
UNION ALL
(SELECT Region.strLocalName as RegionName, P.strName as ProductName, P.strProductType, P.curDefaultAmount, SUM(T.intQty) as SumQty, SUM(T.curAmount) as SumAmt FROM tblPersonRegistration_1 as PR INNER JOIN tblEntity as E ON (E.intEntityID = PR.intEntityID) INNER JOIN tblTransactions as T ON (PR.intPersonRegistrationID = T.intPersonRegistrationID) INNER JOIN tblTransLog as TL ON (TL.intLogID = T.intTransLogID) INNER JOIN tblProducts as P ON (P.intProductID=T.intProductID) INNER JOIN tblTempEntityStructure as TS ON (intChildLevel=20 and TS.intChildID = PR.intEntityID) INNER JOIN tblEntity as Region ON (Region.intEntityID= intChildID AND intChildLevel=20) WHERE T.intStatus=1 AND P.intProductNationalPeriodID=121 AND PR.intNationalPeriodID=121 GROUP BY RegionName, P.intProductID)


	],
	StatsReport => 0,
	ReportEntity => 100,
	ReportLevel => 0,
	Template => 'default',
	DistinctValues => 1,
	Labels => [
		['RegionName','Region'],
		['ProductName','Product'],
		['ProductName','Product Type'],
		['curDefaultAmount','Product Cost'],
		['SumQty','Quantity Purchased'],
		['SumAmt','Total'],
	],
	Sort => [
		['RegionName','ASC','string'],
		['ProductName','ASC','string'],
	],
}
